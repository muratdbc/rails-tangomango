<div class="container" >
  <div class="table-responsive container-fluid col-md-offset-3 col-md-6">
    <table class="table table-striped ">
      <table>
        <td>
          <% @events.each do |event|%>
          <tr>
            <td ><h4>
              <%= event.title%></td>
            </h4>
            <td><h4><%= event.date %></td>
          </h4>
          <td>
            <button class="btn btn-success"><%= link_to 'Edit',edit_event_path(event) %> </button>
          </td>
          <td>

            <button class="btn"><%= link_to 'Destroy', event_path(event),
            method: :delete,
            data: { confirm: 'Are you sure?' } %></button>
          </td>
        </tr>
        <% end %>
      </td>
    </table>
  </div>
</div>
</div>